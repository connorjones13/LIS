RewriteEngine On

RewriteBase /item
RewriteRule ^damaged/([0-9]+)/$         damaged.php?id=$1           [L]
RewriteRule ^lost/([0-9]+)/$            lost.php?id=$1              [L]
RewriteRule ^reserve/([0-9]+)/$         reserve.php?id=$1           [L]
RewriteRule ^available/([0-9]+)/$       available.php?id=$1         [L]
RewriteRule ^([0-9]+)/$                 index.php?id=$1             [L]